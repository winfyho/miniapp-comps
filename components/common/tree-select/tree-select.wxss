/* components/common/tree-select/tree-select.wxss */
.tree-select{
    width: 100%;
    height: 100vh;
    display: flex;
}

.tree{
    width: 40%;
    height: 100%;
    background: #f4f4f4;
}

.select{
    width: 70%;
    height: 100%;
}

.select-list{
    width: 100%;
    background: #f5f5f5;
}

.select-list .select-item{
    width: 100%;
    box-sizing: border-box;
    padding: 20rpx;
    height: 400rpx;
    margin-bottom: 15rpx;
    background: #fff;
}

